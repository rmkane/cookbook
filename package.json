{
  "name": "cookbook",
  "version": "1.0.0",
  "description": "A simple recipe app using typescript and sequelize",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "dev": "nodemon src/index.ts",
    "build": "npx tsc",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\""
  },
  "keywords": [
    "sequelize",
    "typescript",
    "food",
    "recipes",
    "cache",
    "speed",
    "microdiff"
  ],
  "author": "ibywaks",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "microdiff": "^1.3.0",
    "mysql2": "^3.6.1",
    "node-cache": "^5.1.2",
    "sequelize": "^6.6.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.171",
    "@types/node": "^20.6.0",
    "@types/node-cache": "^4.2.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.1.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.0.0",
    "typescript": "^5.2.2"
  }
}
